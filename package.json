{
  "name": "kurt-nelson",
  "version": "1.0.0",
  "private": true,
  "description": "Kurt Nelson",
  "author": "Kurt Nelson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "postinstall": "husky install",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public"
  },
  "dependencies": {
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^4.16.0",
    "gatsby-cli": "^4.16.0",
    "gatsby-plugin-gatsby-cloud": "^4.16.0",
    "gatsby-plugin-google-analytics": "^4.16.0",
    "gatsby-plugin-image": "^2.16.0",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-mdx": "^3.16.0",
    "gatsby-plugin-offline": "^5.16.0",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-sharp": "^4.16.0",
    "gatsby-plugin-sitemap": "^5.16.0",
    "gatsby-plugin-styled-components": "^5.16.0",
    "gatsby-remark-images": "^6.16.0",
    "gatsby-source-filesystem": "^4.16.0",
    "gatsby-transformer-sharp": "^4.16.0",
    "gh-pages": "^4.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx-frontmatter": "^1.1.1",
    "styled-components": "^5.3.5"
